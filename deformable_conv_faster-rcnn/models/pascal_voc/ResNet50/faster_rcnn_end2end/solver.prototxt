train_net: "models/pascal_voc/ResNet50/faster_rcnn_end2end/train.prototxt"
base_lr: 0.0005
lr_policy: "step"
gamma: 0.1
#~=4.78
stepsize: 50000
display: 20
average_loss: 100
# iter_size: 1
momentum: 0.9
weight_decay: 0.0005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 0
#clip_gradients: 100
# We still use the snapshot prefix, though
snapshot_prefix: "ResNet50_dcn"
iter_size: 2
